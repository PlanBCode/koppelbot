{
  "_": {
    "connector": {
      "type": "file",
      "parse": "json",
      "path": "data/accounts.json"
    }
  },
  "email": {
    "type": "string",
    "regex": "^TODO$",
    "default": "hello@default.com",
    "connector": {
      "key": "key"
    },
    "access" : {
      "read" : ["guest"]
    }
  },
  "password": {
    "type": "password"
  },
  "username": {
    "type": "string",
    "minLength": 3
  },
  "groups" : {
    "type" : "array",
    "default" : ["guest"],
    "subType" : {
      "type" : "reference",
      "uri": "/group/*/name"
    }
  }
}
