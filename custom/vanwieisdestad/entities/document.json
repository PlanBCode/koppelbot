{
  "_": {
    "connector": {
      "type": "sqlite",
      "path": "data/mapdata.sqlite",
      "table": "Documenten"
    },
    "access" : {
      "read" : ["/group/guest"]
    }
  },
  "id": {
    "type": "id",
    "autoIncrement": true,
    "connector": {
      "key": "ID"
    }
  },
  "Type" : {
    "type": "string",
    "connector": {
      "key": "Type"
    }
  },
  "Bron" : {
    "title":true,
    "type": "string",
    "connector": {
      "key": "Bron"
    }
  },
  "Datum" : {
    "type": "string",
    "connector": {
      "key": "Datum"
    }
  },
  "Bestand" : {
    "title":true,
    "type": "string",
    "connector": {
      "key": "Bestand"
    }
  },
  "EigenaarID" : {
    "type": "reference",
    "uri": "/eigenaar/*",
    "connector": {
      "key": "EigenaarID"
    }
  },
  "BRK" : {
    "type": "geojson",
    "stringify": true,
    "connector": {
      "key": "BRK"
    }
  },
  "BAG" : {
    "type": "geojson",
    "stringify": true,
    "connector": {
      "key": "BAG"
    }
  },
  "Lijst" : {
    "type": "string",
    "connector": {
      "key": "Lijst"
    }
  }
}
