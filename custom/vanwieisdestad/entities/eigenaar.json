{
  "_": {
    "connector": {
      "type": "sqlite",
      "path": "data/mapdata.sqlite",
      "table": "Eigenaren"
    },
    "access" : {
      "read" : ["guest"]
    }
  },
  "id": {
    "type": "id",
    "autoIncrement": true,
    "connector": {
      "key": "ID"
    }
  },
  "aantal": {
    "type": "number",
    "connector": {
      "key": "Aantal"
    }
  },
  "oppervlakte": {
    "type": "number",
    "connector": {
      "key": "Oppervlakte"
    }
  },
  "name" : {
    "displayName": "Naam",
    "title": true,
    "type": "string",
    "minLength": 1,
    "connector": {
      "key": "Naam"
    }
  }
}
