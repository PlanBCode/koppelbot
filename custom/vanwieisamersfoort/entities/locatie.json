{
  "_": {
    "connector": {
      "type": "sqlite",
      "path": "data/mapdata.sqlite",
      "table": "Locaties"
    },
    "access" : {
      "read" : ["guest"]
    }
  },
  "id": {
    "type": "id",
    "autoIncrement": true,
    "connector": {
      "key": "ID"
    }
  },
  "Type" : {
    "type": "string",
    "connector": {
      "key": "type"
    }
  },
  "geojson" : {
    "type": "geojson",
    "stringify": true,
    "connector": {
      "key": "geojson"
    }
  },
  "DocumentID" : {
    "type": "reference",
    "uri": "/document/*",
    "connector": {
      "key": "DocumentID"
    }
  },
  "Oppervlakte" : {
    "type": "number",
    "unit": "m^2",
    "connector": {
      "key": "geojson.properties.kadastraleGrootteWaarde"
    }
  },
  "Eigenschappen" : {
    "type": "json",
    "stringify": true,
    "connector": {
      "key": "geojson.properties"
    }
  },
  "EigenaarID" : {
    "type": "reference",
    "uri": "/eigenaar/*",
    "connector": {
      "key": "EigenaarID"
    }
  }

}
