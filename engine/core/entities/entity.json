{
  "_": {
    "connector": {
      "type": "directory",
      "parse": "json",
      "path": "custom/*/entities/:engine/core/entities/",
      "extension": "json"
    },
    "access": {
      "read": [
        "/group/guest"
      ]
    }
  },
  "id": {
    "type": "string",
    "index": "true",
    "connector": {
      "key": "key"
    }
  },
  "content": {
    "type": "object",
    "subType": {
      "type": "type"
    },
    "connector": {
      "key": "content"
    }
  }
}
