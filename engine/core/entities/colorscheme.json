{
  "_": {
    "connector": {
      "type": "querystorage",
      "root" : "colorscheme"
    },
    "access": {
      "write": [
        "guest"
      ]
    }
  },
  "id": {
    "type": "id",
    "connector": {
      "index": true,
      "key": "key"
    }
  },
  "name": {
    "connector": {
      "key": "value"
    },
    "type": "string"
  }
}
